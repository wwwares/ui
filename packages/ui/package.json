{
	"name": "@wwwares/ui-react",
	"version": "0.4.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"sideEffects": false,
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --external react,@wwwares/ui-system",
		"dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react,@wwwares/ui-system",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.0",
		"react": "^18.3.1",
		"tsup": "8.3.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@tabler/icons-react": "^3.35.0",
		"@wwwares/ui-system": "workspace:0.4.0",
		"clsx": "^2.1.1",
		"react-aria-components": "^1.12.0"
	}
}
